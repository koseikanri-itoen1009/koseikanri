      SQLType="NUMERIC" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="LineNo"
      IsPersistent="false"
      IsNotNull="true"
      Precision="5"
      Scale="0"
      Type="oracle.jbo.domain.Number"
      ColumnType="NUMBER"
      AliasName="LINE_NO"
      Expression="LINE_NO"
      SQLType="NUMERIC" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="ItemNo"
      IsPersistent="false"
      IsNotNull="true"
      Precision="32"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="ITEM_NO"
      Expression="ITEM_NO"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="32" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="ItemName"
