<?xml version="1.0" encoding='Windows-31J'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="BatchNoVO"
   OrderBy="PLANT_CODE,BATCH_NO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT gbh.batch_no    batch_no
      ,gbh.plant_code  plant_code
      ,gbh.batch_id    batch_id
FROM   gme_batch_header gbh
      ,sy_orgn_usr      sou
WHERE  gbh.plant_code = sou.orgn_code
AND    gbh.attribute4 IN('5', '6', '7')
AND    sou.user_id    = fnd_global.user_id
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.14.00" />
      <Attr Name="_CodeGenFlagNew" Value="4" />
   </DesignTime>
   <ViewAttribute
      Name="BatchNo"
      IsPersistent="false"
      IsNotNull="true"
      Precision="32"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="BATCH_NO"
      Expression="BATCH_NO"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="32" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="PlantCode"
      IsPersistent="false"
      IsNotNull="true"
      Precision="4"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="PLANT_CODE"
      Expression="PLANT_CODE"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="4" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="BatchId"
      IsPersistent="false"
      IsNotNull="true"
      Precision="10"
      Scale="0"
      Type="oracle.jbo.domain.Number"
      ColumnType="NUMBER"
      AliasName="BATCH_ID"
      Expression="BATCH_ID"
      SQLType="NUMERIC" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
</ViewObject>
