<ViewObject
   Name="XxwshSearchVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="itoen.oracle.apps.xxwsh.xxwsh920002j.server.XxwshSearchVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   Passivate="All"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT ximv.item_id,
       ximv.lot_ctl,
       ximv.item_short_name,
       ximv.num_of_cases,
       ximv.num_of_deliver,
       ximv.conv_unit,
       xicv.prod_class_code prod_class,
       xicv.item_class_code item_class,
      (CASE
         WHEN ((:0 = 1) AND (xicv.item_class_code = '5')
                        AND (xicv.prod_class_code IN ('1', '2'))
                        AND (ximv.conv_unit IS NOT NULL))
         THEN ximv.conv_unit
         WHEN ((:1 = 3) AND (xicv.item_class_code = '5')
                        AND (xicv.prod_class_code = '2')
                        AND (ximv.conv_unit IS NOT NULL))
         THEN ximv.conv_unit
         ELSE ximv.item_um
       END) uom_code,
      (CASE
         WHEN ((:2 = 1) AND (xicv.item_class_code = '5')
                        AND (xicv.prod_class_code IN ('1', '2'))
                        AND (ximv.conv_unit IS NOT NULL))
         THEN TO_CHAR(NVL((:3 / TO_NUMBER(ximv.num_of_cases)),0),'FM999,999,990.000')
         WHEN ((:4 = 3) AND (xicv.item_class_code = '5')
                        AND (xicv.prod_class_code = '2')
                        AND (ximv.conv_unit IS NOT NULL))
         THEN TO_CHAR(NVL((:5 / TO_NUMBER(ximv.num_of_cases)),0),'FM999,999,990.000')
         ELSE TO_CHAR(NVL(:6,0),'FM999,999,990.000')
       END) quantity,
      (CASE
         WHEN ((:7 = 1) AND (xicv.item_class_code = '5')
                        AND (xicv.prod_class_code IN ('1', '2'))
                        AND (ximv.conv_unit IS NOT NULL))
         THEN TO_CHAR(NVL((:8 / TO_NUMBER(ximv.num_of_cases)),0),'FM999,999,990.000')
         WHEN ((:9 = 3) AND (xicv.item_class_code = '5')
                        AND (xicv.prod_class_code = '2')
                        AND (ximv.conv_unit IS NOT NULL))
         THEN TO_CHAR(NVL((:10 / TO_NUMBER(ximv.num_of_cases)),0),'FM999,999,990.000')
         ELSE TO_CHAR(NVL(:11,0),'FM999,999,990.000')
       END) sum_reserved_quantity,
      (CASE
         WHEN ((:12 = 1) AND (xicv.item_class_code = '5')
                        AND (xicv.prod_class_code IN ('1','2'))
                        AND (ximv.conv_unit IS NOT NULL))
         THEN '1'
         WHEN ((:13 = 3) AND (xicv.item_class_code = '5')
                         AND (xicv.prod_class_code = '2')
                         AND (ximv.conv_unit IS NOT NULL))
         THEN '1'
         ELSE '0'
       END) conv_unit_use_kbn
  FROM xxcmn_item_mst2_v        ximv,
       xxcmn_item_categories5_v xicv
 WHERE ximv.item_no = :14
   AND :15          BETWEEN ximv.start_date_active
                    AND     ximv.end_date_active
   AND xicv.item_id = ximv.item_id
   ]]></SQLQuery>
