<?xml version="1.0" encoding='Windows-31J'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="OrderNumberVO"
   OrderBy="HEADER_NUMBER"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="oracle.apps.fnd.framework.server.OAViewObjectImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT pha.segment1 header_number      -- 発注No
      ,pha.po_header_id header_id      -- 発注ヘッダID
      ,pha.attribute5 location_code    -- 納入先コード
FROM po_headers_all pha            -- 発注ヘッダ
WHERE pha.attribute1 >= '20'
  AND pha.attribute1 != '99'
  AND xxcmn_common_pkg.get_opminv_close_period < TO_CHAR(FND_DATE.STRING_TO_DATE(pha.attribute4, 'YYYY/MM/DD'), 'YYYYMM')
   ]]></SQLQuery>
   <DesignTime>
      <Attr Name="_version" Value="9.0.3.14.00" />
      <Attr Name="_CodeGenFlagNew" Value="4" />
   </DesignTime>
   <ViewAttribute
      Name="HeaderNumber"
      IsPersistent="false"
      IsNotNull="true"
      Precision="20"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="HEADER_NUMBER"
      Expression="HEADER_NUMBER"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="20" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="HeaderId"
      IsPersistent="false"
      IsNotNull="true"
      Type="oracle.jbo.domain.Number"
      ColumnType="NUMBER"
      AliasName="HEADER_ID"
      Expression="HEADER_ID"
      SQLType="NUMERIC" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="22" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="LocationCode"
      IsPersistent="false"
      Precision="150"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="LOCATION_CODE"
      Expression="LOCATION_CODE"
      SQLType="VARCHAR" >
      <DesignTime>
         <Attr Name="_DisplaySize" Value="150" />
      </DesignTime>
   </ViewAttribute>
   <ViewAttribute
      Name="PeopleCode"
      IsQueriable="false"
      IsPersistent="false"
      Precision="255"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="VIEW_ATTR"
      Passivate="true"
      SQLType="VARCHAR" >
   </ViewAttribute>
   <ViewAttribute
      Name="PurchaseSiteCode"
      IsQueriable="false"
      IsPersistent="false"
      Precision="255"
      Type="java.lang.String"
      ColumnType="VARCHAR2"
      AliasName="VIEW_ATTR"
      Passivate="true"
      SQLType="VARCHAR" >
   </ViewAttribute>
   <AttrArray Name="KeyAttributes">
      <Item Value="HeaderNumber" />
   </AttrArray>
</ViewObject>
