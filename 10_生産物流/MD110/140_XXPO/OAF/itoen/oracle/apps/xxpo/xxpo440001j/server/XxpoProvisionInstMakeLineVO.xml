<ViewObject
   Name="XxpoProvisionInstMakeLineVO"
   OrderBy="ORDER_LINE_NUMBER"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="itoen.oracle.apps.xxpo.xxpo440001j.server.XxpoProvisionInstMakeLineVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 'N'                              record_type       -- レコードタイプ「N:更新」固定
      ,xola.order_header_id             order_header_id   -- 受注ヘッダアドオンID
      ,xola.order_line_id               order_line_id     -- 受注明細アドオンID
      ,xola.order_line_number           order_line_number -- 明細番号
      ,ximv.item_id                     item_id           -- OPM品目ID
      ,xola.shipping_item_code          item_no           -- 品目コード
      ,xola.shipping_inventory_item_id  inv_item_id       -- INV品目ID
      ,ximv.item_short_name             item_name         -- 品目名称(略称)
      ,xola.futai_code                  futai_code        -- 付帯コード
      ,TO_CHAR(xola.based_request_quantity, 'FM999999990.000')
                                        req_quantity      -- 依頼数量
      ,TO_CHAR(xola.quantity, 'FM999,999,990.000')
                                        inst_quantity     -- 指示数量
      ,ximv.item_um                     item_um           -- 単位
      ,TO_CHAR(xola.unit_price, 'FM9,999,990.00')
                                        unit_price        -- 単価
      ,TO_CHAR(ROUND(NVL(xola.shipped_quantity, NVL(xola.quantity, 0)) * NVL(xola.unit_price, 0)), 'FM9,999,999,999,990')
                                        amount            -- 金額
      ,TO_CHAR(xola.shipped_quantity, 'FM999,999,990.000')
                                        shipped_quantity  -- 出荷実績数量
      ,TO_CHAR(xola.ship_to_quantity, 'FM999,999,990.000')
                                        ship_to_quantity  -- 入庫実績数量
      ,xola.line_description            line_description  -- 備考(摘要)
      ,ximv.conv_unit                   conv_unit         -- 入出庫換算単位
      ,ximv.num_of_deliver              num_of_deliver    -- 出荷入数
      ,ximv.num_of_cases                num_of_cases      -- ケース入数
      ,ximv.whse_item_id                whse_item_id      -- 倉庫品目ID
      ,NVL(whximv.inventory_item_id, ximv.inventory_item_id)
                                        whse_inv_item_id  -- 倉庫INV品目ID
      ,NVL(whximv.item_no, ximv.item_no)
                                        whse_item_no      -- 倉庫品目コード
      ,CASE
         WHEN ( xoha.req_status = '05')
           THEN 'ItemNo'
         WHEN ((xoha.req_status = '06') AND ('15' <> :1))
           THEN 'ItemNo'
         WHEN ((xoha.req_status = '07') AND (:2 IN ('11', '13')) AND (xola.shipped_quantity  IS NULL)  -- 出庫実績無
                                                                 AND (xola.ship_to_quantity  IS NULL)  -- 入庫実績無
                                                                 AND (xola.reserved_quantity IS NULL)  -- 未引当
                                                                 AND (xoha.po_no             IS NULL)) -- 未発注
           THEN 'ItemNo'
         ELSE 'ItemDisable'
       END                            item_switcher     -- 品目制御
      ,CASE
         WHEN ('12' = :3)
           THEN 'FutaiDisable'
         WHEN ( xoha.req_status = '05')
           THEN 'FutaiCode'
         WHEN ((xoha.req_status = '06') AND ('15' <> :4))
           THEN 'FutaiCode'
         WHEN ((xoha.req_status = '07') AND (:5 IN ('11', '13')) AND (xola.shipped_quantity  IS NULL)  -- 出庫実績無
                                                                 AND (xola.ship_to_quantity  IS NULL)  -- 入庫実績無
                                                                 AND (xoha.delivery_no       IS NULL)  -- 未配車
                                                                 AND (xola.reserved_quantity IS NULL)  -- 未引当
                                                                 AND (xoha.po_no             IS NULL)) -- 未発注
           THEN 'FutaiCode'
         ELSE 'FutaiDisable'
       END                            futai_switcher     -- 付帯制御
      ,CASE
         WHEN ((xoha.req_status = '06') AND ('15' = :6))
           THEN 'ReqDisable'
         WHEN (xoha.req_status IN ('07', '08'))
           THEN 'ReqDisable'
         ELSE 'ReqQuantity'
       END                            req_switcher       -- 依頼数制御
      ,CASE
         WHEN ((xoha.req_status = '07') AND ('12' = :7))
           THEN 'LineDescriptionDisable'
         WHEN (xoha.req_status = '08')
           THEN 'LineDescriptionDisable'
         ELSE 'LineDescription'
       END                            desc_switcher      -- 備考制御
      ,CASE
         WHEN ((xoha.req_status = '08') AND ('12' <> :8))
           THEN 'ShippedIconEnable'
         WHEN ((xoha.req_status = '07') AND (:9 IN ('11', '13', '15')) AND (xola.shipped_quantity IS NOT NULL)) -- 出庫実績有
           THEN 'ShippedIconEnable'
         ELSE 'ShippedIconDisable'
       END                            shipped_switcher  -- 出庫実績アイコン
      ,CASE
         WHEN ((xoha.req_status = '08') AND ('12' <> :10))
           THEN 'ShipToIconEnable'
         WHEN ((xoha.req_status = '07') AND (:11 IN ('11', '13', '15')) AND (xola.ship_to_quantity IS NOT NULL)) -- 入庫実績有
           THEN 'ShipToIconEnable'
         ELSE 'ShipToIconDisable'
       END                            ship_to_switcher  -- 入庫実績アイコン
      ,CASE
         WHEN ((xoha.req_status = '07') AND (:12 IN ('11', '13', '15')) AND (   (xola.shipped_quantity IS NULL)  -- 出庫実績有
                                                                             OR (xola.ship_to_quantity IS NULL)) -- 入庫実績有
                                                                        AND (xoha.po_no IS NULL))                -- 発注済
           THEN 'ReserveIconEnable'
         WHEN ((xoha.req_status = '08') AND (:13 IN ('11', '13', '15')) AND (xola.shipped_quantity IS NULL)  -- 出庫実績有
                                                                        AND (xola.ship_to_quantity IS NULL)) -- 入庫実績有
           THEN 'ReserveIconEnable'
         ELSE 'ReserveIconDisable'
       END                            reserve_switcher  -- 引当アイコン
      ,CASE
         WHEN (xoha.req_status IN ('05', '06'))
           THEN 'DeleteEnable'
         WHEN ((xoha.req_status = '07') AND (:14 IN ('11', '13', '15')) AND (xola.shipped_quantity  IS NULL)  -- 出庫実績無
                                                                        AND (xola.ship_to_quantity  IS NULL)  -- 入庫実績無
                                                                        AND (xoha.delivery_no       IS NULL)  -- 未配車
                                                                        AND (xola.reserved_quantity IS NULL)  -- 未引当
                                                                        AND (xoha.po_no             IS NULL)) -- 未発注
           THEN 'DeleteEnable'
         ELSE 'DeleteDisable'
       END                              delete_switcher   -- 削除アイコン
      ,TO_CHAR(ximv.lot_ctl)            lot_ctl           -- ロット管理区分
      ,xola.unit_price                  unit_price_num    -- 単価(Number)
      ,TO_CHAR(xola.weight)             weight            -- 重量
      ,TO_CHAR(xola.capacity)           capacity          -- 容積
/*    DB項目     */
      ,ximv.item_id                     db_item_id          -- (DB)OPM品目ID
      ,ximv.item_no                     db_item_no          -- (DB)OPM品目No
      ,xola.futai_code                  db_futai_code       -- (DB)付帯コード
      ,TO_CHAR(xola.based_request_quantity, 'FM999999990.000')
                                        db_req_quantity     -- (DB)依頼数量
      ,xola.line_description            db_line_description -- (DB)備考(摘要)
      ,TO_CHAR(xola.quantity, 'FM999,999,990.000')
                                        db_inst_quantity    -- 指示数量
      ,xola.unit_price                  db_unit_price_num   -- (DB)単価(Number)
FROM   xxwsh_order_headers_all xoha   -- 受注ヘッダアドオン
      ,xxwsh_order_lines_all   xola   -- 受注明細アドオン
      ,xxcmn_item_mst2_v       ximv   -- OPM品目情報VIEW2
      ,xxcmn_item_mst_v        whximv -- OPM品目情報VIEW(倉庫品目用)
WHERE  
/* 結合条件 */
       xoha.order_header_id             = xola.order_header_id
AND    xola.shipping_inventory_item_id  = ximv.inventory_item_id
AND    ximv.whse_item_id                = whximv.item_id(+)
/* 抽出条件 */
AND    xola.delete_flag     = 'N' -- 未削除
AND    xola.order_header_id = :15
AND    NVL(xoha.shipped_date, xoha.schedule_ship_date) BETWEEN ximv.start_date_active
                                                       AND     ximv.end_date_active
   ]]></SQLQuery>
