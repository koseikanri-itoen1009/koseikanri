<?xml version="1.0" encoding='Windows-31J'?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XxpoProvisionRtnMakeTotalVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="oracle.apps.fnd.framework.server.OAViewRowImpl"
   ComponentClass="itoen.oracle.apps.xxpo.xxpo443001j.server.XxpoProvisionRtnMakeTotalVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT xola.order_header_id              order_header_id -- 受注ヘッダアドオンID
      ,TO_CHAR(SUM(NVL(xola.quantity, 0)), 'FM999,999,990.000') inst_quantity_total -- 指示数量合計
      ,TO_CHAR(  
         SUM(  
           ROUND(  
             NVL(xola.shipped_quantity, NVL(xola.quantity, 0)) * NVL(xola.unit_price, 0)  
           )  
         ),'FM9,999,999,999,990'  
       ) amount_total -- 金額
      ,TO_CHAR(SUM(NVL(xola.shipped_quantity, 0)), 'FM999,999,990.000') shipped_quantity_total -- 出荷実績数量
      ,TO_CHAR(SUM(NVL(xola.ship_to_quantity, 0)), 'FM999,999,990.000') ship_to_quantity_total -- 入庫実績数量
FROM   xxwsh_order_lines_all xola -- 受注明細アドオン
WHERE  xola.delete_flag     = 'N'  -- 未削除
AND    xola.order_header_id = :1  
GROUP BY xola.order_header_id
   ]]></SQLQuery>
