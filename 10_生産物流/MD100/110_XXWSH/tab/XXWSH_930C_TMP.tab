CREATE TABLE xxwsh.xxwsh_930c_tmp
  (
    location_code             VARCHAR2(4)
   ,location_name             VARCHAR2(20)
   ,ship_date                 DATE
   ,arvl_date                 DATE
   ,head_sales_code           VARCHAR2(4)
   ,head_sales_name           VARCHAR2(20)
   ,deliver_code              VARCHAR2(9)
   ,deliver_name              VARCHAR2(60)
   ,career_code               VARCHAR2(4)
   ,career_name               VARCHAR2(20)
   ,ship_method_code          VARCHAR2(2)
   ,ship_method_name          VARCHAR2(14)
   ,ship_type                 VARCHAR2(4)
   ,delivery_no               VARCHAR2(12)
   ,request_no                VARCHAR2(12)
   ,item_code                 VARCHAR2(7)
   ,item_name                 VARCHAR2(20)
   ,lot_no                    VARCHAR2(10)
   ,product_date              DATE
   ,use_by_date               DATE
   ,original_char             VARCHAR2(6)
   ,lot_status                VARCHAR2(10)
   ,quant_r                   VARCHAR2(12)
   ,quant_i                   VARCHAR2(12)
   ,quant_o                   VARCHAR2(12)
   ,reason                    VARCHAR2(6)
   ,inst_rslt_div             VARCHAR2(1)
   ,created_by                NUMBER(15)    NOT NULL
   ,creation_date             DATE          NOT NULL
   ,last_updated_by           NUMBER(15)    NOT NULL
   ,last_update_date          DATE          NOT NULL
   ,last_update_login         NUMBER(15)
   ,request_id                NUMBER(15)
   ,program_application_id    NUMBER(15)
   ,program_id                NUMBER(15)
   ,program_update_date       DATE
  ) ;
--
COMMENT ON TABLE xxwsh.xxwsh_930c_tmp IS '帳票用中間テーブル（93C）' ;
COMMENT ON COLUMN xxwsh.xxwsh_930c_tmp.location_code            IS '出庫倉庫コード' ;
