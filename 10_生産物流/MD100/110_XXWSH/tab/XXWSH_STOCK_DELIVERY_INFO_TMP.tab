    best_bfr_date               DATE,
    lot_sign                    VARCHAR2(6),
    --lot_quantity                NUMBER(12,3),
    lot_quantity                NUMBER,
    new_modify_del_class        NUMBER(1,0),
    update_date                 DATE,
    line_number                 NUMBER,
    data_type                   VARCHAR2(1),
    eos_shipped_locat           VARCHAR2(4),
    eos_freight_carrier         VARCHAR2(4),
    eos_csv_output              VARCHAR2(4),
    notif_date                  DATE,
    target_request_id           NUMBER(15,0)
