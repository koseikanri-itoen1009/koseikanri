CREATE TABLE XXCMM.XXCMM_WK_ITEMRSV_CSV
(
    ITEM_ID                        NUMBER NOT NULL,
    ITEM_CODE                      VARCHAR2(40) NOT NULL,
    ITEM_NAME                      VARCHAR2(60),
    APPLY_DATE                     DATE,
    ITEM_STATUS                    NUMBER(2,0),
    ITEM_STATUS_NAME               VARCHAR2(80),
    FIXED_PRICE                    NUMBER,
    CMPNT_COST                     NUMBER,
    CMP_COST_APPLY_DATE            DATE,
    DISCRETE_COST                  NUMBER,
    POLICY_GROUP                   VARCHAR2(4),
    POLICY_GRP_NAME                VARCHAR2(80),
    CREATED_BY                     NUMBER(15,0) NOT NULL,
    CREATION_DATE                  DATE NOT NULL,
    LAST_UPDATED_BY                NUMBER(15,0) NOT NULL,
    LAST_UPDATE_DATE               DATE NOT NULL,
    LAST_UPDATE_LOGIN              NUMBER(15,0),
    REQUEST_ID                     NUMBER(15,0) NOT NULL,
    PROGRAM_APPLICATION_ID         NUMBER(15,0),
    PROGRAM_ID                     NUMBER(15,0),
    PROGRAM_UPDATE_DATE            DATE
)
/
COMMENT ON TABLE XXCMM.XXCMM_WK_ITEMRSV_CSV IS '品目一覧作成ワーク（予約設定値）'
/
COMMENT ON COLUMN XXCMM.XXCMM_WK_ITEMRSV_CSV.ITEM_ID IS '品目ID'
/
COMMENT ON COLUMN XXCMM.XXCMM_WK_ITEMRSV_CSV.ITEM_CODE IS '品名コード'
/
COMMENT ON COLUMN XXCMM.XXCMM_WK_ITEMRSV_CSV.ITEM_NAME IS '正式名'
/
COMMENT ON COLUMN XXCMM.XXCMM_WK_ITEMRSV_CSV.APPLY_DATE IS '適用開始日'
/
COMMENT ON COLUMN XXCMM.XXCMM_WK_ITEMRSV_CSV.ITEM_STATUS IS '品目ステータス'
/
COMMENT ON COLUMN XXCMM.XXCMM_WK_ITEMRSV_CSV.ITEM_STATUS_NAME IS '品目ステータス名'
/
COMMENT ON COLUMN XXCMM.XXCMM_WK_ITEMRSV_CSV.FIXED_PRICE IS '定価'
/
COMMENT ON COLUMN XXCMM.XXCMM_WK_ITEMRSV_CSV.CMPNT_COST IS '標準原価計'
/
COMMENT ON COLUMN XXCMM.XXCMM_WK_ITEMRSV_CSV.CMP_COST_APPLY_DATE IS '標準原価適用開始日'
