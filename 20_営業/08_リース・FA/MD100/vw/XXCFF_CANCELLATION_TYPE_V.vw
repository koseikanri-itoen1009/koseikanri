CREATE OR REPLACE VIEW XXCFF_CANCELLATION_TYPE_V (
 CANCELLATION_TYPE_CODE    -- 解約区分コード
,CANCELLATION_TYPE_NAME    -- 解約区分名称
,ENABLED_FLAG          -- 有効フラグ
,START_DATE_ACTIVE     -- 開始日
,END_DATE_ACTIVE       -- 終了日
,FORM_OBJ_INS_FLAG     -- 物件登録画面使用フラグ
)
AS
SELECT
 FLV.LOOKUP_CODE AS CANCELLATION_TYPE_CODE
,FLV.DESCRIPTION AS CANCELLATION_TYPE_NAME
,FLV.ENABLED_FLAG AS ENABLED_FLAG
,FLV.START_DATE_ACTIVE AS START_DATE_ACTIVE
,FLV.END_DATE_ACTIVE AS END_DATE_ACTIVE
,FLV.ATTRIBUTE1 AS FORM_OBJ_INS_FLAG
FROM   FND_LOOKUP_VALUES_VL FLV
WHERE  FLV.LOOKUP_TYPE = 'XXCFF1_CANCELLATION_TYPE'
;
COMMENT ON COLUMN XXCFF_CANCELLATION_TYPE_V.CANCELLATION_TYPE_CODE IS '解約区分コード';
COMMENT ON COLUMN XXCFF_CANCELLATION_TYPE_V.CANCELLATION_TYPE_NAME IS '解約区分名称';
COMMENT ON COLUMN XXCFF_CANCELLATION_TYPE_V.ENABLED_FLAG IS '有効フラグ';
COMMENT ON COLUMN XXCFF_CANCELLATION_TYPE_V.START_DATE_ACTIVE IS '開始日';
COMMENT ON COLUMN XXCFF_CANCELLATION_TYPE_V.END_DATE_ACTIVE IS '終了日';
COMMENT ON COLUMN XXCFF_CANCELLATION_TYPE_V.FORM_OBJ_INS_FLAG IS '物件登録画面使用フラグ';
COMMENT ON TABLE XXCFF_CANCELLATION_TYPE_V IS '解約区分ビュー';
