COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.lease_company                            IS 'リース会社';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.first_charge                             IS '初回月額リース料_リース料';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.first_tax_charge                         IS '初回消費税額_リース料';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.second_charge                            IS '2回目以降月額リース料_リース料';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.second_tax_charge                        IS '2回目以降消費税額_リース料';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.first_deduction                          IS '初回月額リース料_控除額';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.first_tax_deduction                      IS '初回月額消費税額_控除額';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.estimated_cash_price                     IS '見積現金購入価額';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.life_in_months                           IS '法定耐用年数';
