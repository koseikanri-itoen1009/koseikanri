COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.first_installation_place                 IS '初回設置先';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.file_id                                  IS 'ファイルID';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.created_by                               IS '作成者';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.creation_date                            IS '作成日';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.last_updated_by                          IS '最終更新者';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.last_update_date                         IS '最終更新日';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.last_update_login                        IS '最終更新ログイン';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.request_id                               IS '要求ID';
COMMENT ON COLUMN xxcff.xxcff_cont_lines_work.program_application_id                   IS 'コンカレント・プログラム・アプリケーションID';
