  fi
  if [ -f ${C_std_out2} ]
  then
    rm ${C_std_out2}
    echo "Temporay file2 Delete"
    echo ""                      #空行
  fi
  #終了メッセージ
  if [ -z "$1" ]
  then
    echo "${L_90004_msg}" >>`echo ${C_ofil}`   #正常終了メッセージ
    exit ${C_exit_norm}
  else
    echo "${L_10008_msg}" >>`echo ${C_ofil}`   #エラー終了メッセージ
    exit ${C_exit_error}
