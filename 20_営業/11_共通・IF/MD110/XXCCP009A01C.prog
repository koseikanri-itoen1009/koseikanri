  then
    L_status_err="${C_10062_msg}"
  else
    L_status_err="${L_status_err}\n${C_10062_msg}"
    fi
  #3) 処理終了
  update_job_process ${L_if_job_status_id_all}
  shell_end ${L_status_err}
fi
