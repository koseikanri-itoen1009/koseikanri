COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.max_days                             IS '最大在庫日数';
COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.shipping_pace                        IS '出荷ペース';
COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.special_yoko_type                    IS '特別横持区分';
