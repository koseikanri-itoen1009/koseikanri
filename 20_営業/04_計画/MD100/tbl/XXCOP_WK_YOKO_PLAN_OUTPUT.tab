COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.receipt_org_code                     IS '移動先倉庫ＣＤ';
COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.receipt_org_name                     IS '移動先倉庫名';
COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.item_id                              IS '品目ＩＤ';
