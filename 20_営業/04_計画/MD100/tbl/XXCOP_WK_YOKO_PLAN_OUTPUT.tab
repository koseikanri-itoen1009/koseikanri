CREATE TABLE XXCOP.XXCOP_WK_YOKO_PLAN_OUTPUT
(
--20090415_Ver1.2_T1_0565_SCS.Goto_MOD_START
--   transaction_id                 NUMBER(6)
   transaction_id                 NUMBER(15)
--20090415_Ver1.2_T1_0565_SCS.Goto_MOD_END
  ,group_id                       NUMBER
  ,shipping_date                  DATE
  ,receipt_date                   DATE
  ,ship_org_code                  VARCHAR2(4)
  ,ship_org_name                  VARCHAR2(50)
  ,receipt_org_code               VARCHAR2(4)
  ,receipt_org_name               VARCHAR2(50)
  ,item_id                        NUMBER
  ,item_no                        VARCHAR2(7)
  ,item_name                      VARCHAR2(50)
  ,freshness_priority             NUMBER
  ,freshness_condition            VARCHAR2(2)
  ,manu_date                      DATE
  ,lot_status                     VARCHAR2(2)
  ,plan_min_qty                   NUMBER
  ,plan_max_qty                   NUMBER
  ,plan_bal_qty                   NUMBER
  ,plan_lot_qty                   NUMBER
  ,delivery_unit                  VARCHAR2(10)
  ,before_stock                   NUMBER
  ,after_stock                    NUMBER
  ,safety_days                    NUMBER
  ,max_days                       NUMBER
  ,shipping_pace                  NUMBER(6)
  ,special_yoko_type              VARCHAR2(1)
  ,supp_bad_type                  VARCHAR2(1)
  ,lot_revers_type                VARCHAR2(1)
  ,earliest_manu_date             DATE
  ,start_manu_date                DATE
  ,created_by                     NUMBER(15)    NOT NULL
  ,creation_date                  DATE          NOT NULL
  ,last_updated_by                NUMBER(15)    NOT NULL
  ,last_update_date               DATE          NOT NULL
  ,last_update_login              NUMBER(15)
  ,request_id                     NUMBER(15)
  ,program_application_id         NUMBER(15)
  ,program_id                     NUMBER(15)
  ,program_update_date            DATE
);
--
COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.transaction_id                       IS '出力ワークテーブルID';
COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.group_id                             IS '横持計画グループID';
