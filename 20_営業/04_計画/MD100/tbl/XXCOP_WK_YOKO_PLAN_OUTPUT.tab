COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.creation_date                        IS '作成日';
COMMENT ON COLUMN xxcop.xxcop_wk_yoko_plan_output.last_updated_by                      IS '最終更新者';
