COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.bill_cust_name             IS '顧客名';
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.location_code              IS '担当拠点コード';
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.location_name              IS '担当拠点名';
