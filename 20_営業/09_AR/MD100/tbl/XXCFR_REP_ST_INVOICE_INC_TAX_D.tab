COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.ship_cust_code             IS '納品先顧客コード';
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.ship_cust_name             IS '納品先顧客名';
--modify 2011.01.17 Ver1.1 Start
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.store_code                 IS '店舗コード';
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.store_code_sort            IS '店舗コード(ソート用)';
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.ship_account_number        IS '納品先顧客コード(ソート用)';
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.invo_account_number        IS '請求用顧客コード(ソート用)';
--modify 2011.01.17 Ver1.1 End
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.slip_date                  IS '伝票日付';
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.slip_num                   IS '伝票No';
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.slip_sum                   IS '伝票金額';
COMMENT ON COLUMN xxcfr.xxcfr_rep_st_invoice_inc_tax_d.slip_tax_sum               IS '伝票税額';
