COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.ship_shop_code            IS '店舗コード';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.sold_location_code        IS '売上拠点コード';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.sold_location_name        IS '売上拠点名';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.sold_area_code            IS '売上拠点本部コード';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.ship_cust_code            IS '納品先顧客コード';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.ship_cust_name            IS '納品先顧客名';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.slip_num                  IS '伝票No';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.delivery_date             IS '納品日';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.ship_amount               IS '金額';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.tax_amount                IS '税額';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.data_empty_message        IS '0件メッセージ';
-- 2014/10/14 Ver1.1 ADD Start
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.output_standard           IS '出力基準';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.inv_amount_no_tax         IS '税抜請求額合計';
COMMENT ON COLUMN xxcfr.xxcfr_rep_invoice_list.tax_amount_sum            IS '税額合計';
-- 2014/10/14 Ver1.1 ADD End
