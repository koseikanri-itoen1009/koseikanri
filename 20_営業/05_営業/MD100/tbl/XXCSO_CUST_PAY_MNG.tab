    ,sub_acct_code                  VARCHAR2(5)
    ,sub_acct_name                  VARCHAR2(240)
    ,payment_amt                    NUMBER        NOT NULL
    ,data_kbn                       VARCHAR2(1)   NOT NULL
    ,pay_start_date                 DATE          NOT NULL
    ,pay_end_date                   DATE
    ,total_amt                      NUMBER(15)    NOT NULL
