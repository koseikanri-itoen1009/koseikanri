    BEGIN ACTIVITY_TRANSITION "*" "XXCSO020002N01"
      ARROW_GEOMETRY = "1;0;0;0;0.500000;-208,-80:-32,-80:"
    END ACTIVITY_TRANSITION
  END PROCESS_ACTIVITY

  BEGIN PROCESS_ACTIVITY "XXCSO020002N01"
