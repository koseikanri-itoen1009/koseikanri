END ACTIVITY

BEGIN ACTIVITY "XXCSO020" "XXCSO020002P01"
  PROTECT_LEVEL = "100"
  CUSTOM_LEVEL = "100"
  DISPLAY_NAME = "XXCSO:ＳＰ専決通知承認プロセス"
  TYPE = "PROCESS"
  RESULT_TYPE = "*"
  RERUN = "RESET"
  COST = "0"
  ICON_NAME = "PROCESS.ICO"
  ERROR_ITEM_TYPE = "WFERROR"
  RUNNABLE = "Y"

  BEGIN PROCESS_ACTIVITY "END"
    ACTIVITY_ITEM_TYPE = "XXCSO020"
    ACTIVITY_NAME = "END"
    START_END = "END"
    ICON_GEOMETRY = "128,-80"
    VALUE_TYPE = "CONSTANT"
  END PROCESS_ACTIVITY

  BEGIN PROCESS_ACTIVITY "START"
    ACTIVITY_ITEM_TYPE = "XXCSO020"
    ACTIVITY_NAME = "START"
    START_END = "START"
    ICON_GEOMETRY = "-208,-80"
    VALUE_TYPE = "ITEMATTR"
