<ViewObject
   Name="XxcsoRsrcPlanSummaryVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="itoen.oracle.apps.xxcso.xxcso019002j.server.XxcsoRsrcPlanSummaryVORowImpl"
   ComponentClass="itoen.oracle.apps.xxcso.xxcso019002j.server.XxcsoRsrcPlanSummaryVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
WITH  tmp
AS
(
  SELECT  :1  AS base_code
         ,:2  AS employee_number
         ,:3  AS target_year_month
         ,TO_CHAR(
            ADD_MONTHS(
              TO_DATE(:4, 'YYYYMM')
             ,1)
           ,'YYYYMM'
          ) AS next_year_month
    FROM DUAL
)
SELECT  tmp.employee_number
       ,tmp.base_code
       ,tmp.target_year_month
       ,SUBSTR(
          tmp.target_year_month
          ,1
          ,4
        ) || '”N' ||
        SUBSTR(
          tmp.target_year_month
          ,5
          ,2
        ) || 'ŒŽ' AS target_year_month_view
       ,tmp.next_year_month
       ,SUBSTR(
          tmp.next_year_month
          ,1
          ,4
        ) || '”N' ||
        SUBSTR(
          tmp.next_year_month
          ,5
          ,2
        ) || 'ŒŽ' AS next_year_month_view
       ,xxcso_rsrc_sales_plans_pkg.get_rsrc_monthly_plan(
          tmp.base_code
         ,tmp.employee_number
         ,tmp.target_year_month
        ) AS target_rsrc_plan
       ,xxcso_rsrc_sales_plans_pkg.get_acct_monthly_plan_sum(
          tmp.base_code
         ,tmp.employee_number
         ,tmp.target_year_month
        ) AS target_rsrc_acct_plan_sum
       ,xxcso_rsrc_sales_plans_pkg.get_rsrc_monthly_differ(
          tmp.base_code
         ,tmp.employee_number
         ,tmp.target_year_month
        ) AS trgt_rsrc_differ
       ,xxcso_rsrc_sales_plans_pkg.get_rsrc_monthly_plan(
          tmp.base_code
         ,tmp.employee_number
         ,tmp.next_year_month
        ) AS next_rsrc_plan
       ,xxcso_rsrc_sales_plans_pkg.get_acct_monthly_plan_sum(
          tmp.base_code
         ,tmp.employee_number
         ,tmp.next_year_month
        ) AS next_rsrc_acct_plan_sum
       ,xxcso_rsrc_sales_plans_pkg.get_rsrc_monthly_differ(
          tmp.base_code
         ,tmp.employee_number
         ,tmp.next_year_month
        ) AS next_rsrc_differ
       ,(CASE 
           WHEN tmp.target_year_month <
                  TO_CHAR(
                    xxcso_util_common_pkg.get_online_sysdate
                   ,'YYYYMM'
                  ) THEN
                    '1'
           ELSE
                    '0'
         END
        ) AS readonly_value
  FROM  tmp
   ]]></SQLQuery>
