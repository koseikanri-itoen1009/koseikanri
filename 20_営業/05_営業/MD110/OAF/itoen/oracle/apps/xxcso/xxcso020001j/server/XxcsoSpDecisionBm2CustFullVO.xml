WHERE XxcsoSpDecisionCustsVEO.SP_DECISION_CUSTOMER_CLASS = '4'
AND   pv.vendor_id(+)                                    = XxcsoSpDecisionCustsVEO.customer_id
AND   pvs.vendor_id(+)                                   = XxcsoSpDecisionCustsVEO.customer_id
