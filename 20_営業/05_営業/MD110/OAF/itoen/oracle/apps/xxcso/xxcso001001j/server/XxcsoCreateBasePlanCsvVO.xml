   Name="XxcsoCreateBasePlanCsvVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="itoen.oracle.apps.xxcso.xxcso001001j.server.XxcsoCreateBasePlanCsvVORowImpl"
   ComponentClass="itoen.oracle.apps.xxcso.xxcso001001j.server.XxcsoCreateBasePlanCsvVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  '1'                        AS  data_kind_flag  -- データ種別（1：拠点用 2：営業員用）
       ,get_ym.period_year         AS  business_year   -- 年度
       ,get_ym.year_month          AS  year_month      -- 年月
       ,null                       AS  slsprsn_xsts_f  -- 営業員有効フラグ
       ,msp.location_cd            AS  base_code       -- 拠点コード
       ,null                       AS  base_name       -- 売上計画検索情報リージョン.拠点名称
       ,TO_CHAR(ROUND((msp.ty_first_total + msp.ty_turn_total) / 1000))    AS  bsc_sls_new_amt                -- 基本新規貢献
       ,TO_CHAR(ROUND(msp.ly_newly_total / 1000))                          AS  bsc_sls_next_amt               -- 基本翌年貢献
       ,TO_CHAR(ROUND(msp.ly_exist_total / 1000))                          AS  bsc_sls_exist_amt              -- 基本既存売上
       ,TO_CHAR(ROUND(msp.discount_total / 1000))                          AS  bsc_discount                   -- 基本値引
       ,TO_CHAR(ROUND((msp.ly_exist_total + msp.ly_newly_total + msp.ty_first_total 
                       + msp.ty_turn_total - msp.discount_total) / 1000))  AS  bsc_total_sls_amt              -- 基本合計売上(基本ノルマ)
       ,TO_CHAR(msp.def_cnt_total)                                         AS  def_cnt_total                  -- 訪問
       ,TO_CHAR(odmp.target_discount_amt)                                  AS  target_discount_amt            -- 目標値引
       ,null  AS  group_number                   -- グループ番号
       ,null  AS  group_leader_flag              -- グループ長区分
       ,null  AS  group_leader_name              -- グループ長名
       ,null  AS  group_grade                    -- グループ内順序
       ,null  AS  employee_number                -- 営業員コード
       ,null  AS  employee_name                  -- 営業員名
       ,null  AS  job_lank                       -- 職位
       ,null  AS  pri_rslt_vd_new_serv_amt       -- 前年実績（VD：新規貢献）
       ,null  AS  pri_rslt_vd_next_serv_amt      -- 前年実績（VD：翌年貢献）
       ,null  AS  pri_rslt_vd_exist_serv_amt     -- 前年実績（VD：既存売上）
       ,null  AS  pri_rslt_new_serv_amt          -- 前年実績（VD以外：新規貢献）
       ,null  AS  pri_rslt_next_serv_amt         -- 前年実績（VD以外：翌年貢献）
       ,null  AS  pri_rslt_exist_serv_amt        -- 前年実績（VD以外：既存売上）
       ,null  AS  bsc_sls_vd_new_serv_amt        -- 基本売上（VD：新規貢献）
       ,null  AS  bsc_sls_vd_next_serv_amt       -- 基本売上（VD：翌年貢献）
       ,null  AS  bsc_sls_vd_exist_serv_amt      -- 基本売上（VD：既存売上）
       ,null  AS  bsc_sls_new_serv_amt           -- 基本売上（VD以外：新規貢献）
       ,null  AS  bsc_sls_next_serv_amt          -- 基本売上（VD以外：翌年貢献）
       ,null  AS  bsc_sls_prsn_total_amt         -- 基本売上（営業員計：計）
       ,null  AS  tgt_sales_vd_new_serv_amt      -- 目標売上（VD：新規貢献）
       ,null  AS  tgt_sales_vd_next_serv_amt     -- 目標売上（VD：翌年貢献）
       ,null  AS  tgt_sales_vd_exist_serv_amt    -- 目標売上（VD：既存売上）
       ,null  AS  tgt_sales_new_serv_amt         -- 目標売上（VD以外：新規貢献）
       ,null  AS  tgt_sales_next_serv_amt        -- 目標売上（VD以外：翌年貢献）
       ,null  AS  tgt_sales_prsn_total_amt       -- 目標売上（営業員計：計）
       ,null  AS  rslt_vd_new_serv_amt           -- 実績（VD：新規貢献）
       ,null  AS  rslt_vd_next_serv_amt          -- 実績（VD：既存売上）
       ,null  AS  rslt_vd_exist_serv_amt         -- 実績（VD：計）
       ,null  AS  rslt_new_serv_amt              -- 実績（VD以外：新規貢献）
       ,null  AS  rslt_next_serv_amt             -- 実績（VD以外：既存売上）
       ,null  AS  rslt_prsn_total_amt            -- 実績（営業員計：計）
       ,null  AS  vis_vd_total_amt               -- 訪問（VD：計）
       ,null  AS  vis_prsn_total_amt             -- 訪問（営業員計：計）
       ,null  AS  sls_pln_effective_flag         -- 計画データ有効フラグ
 FROM    xxcsm_sales_plan  msp                      -- 販売計画テーブル
        ,xxcso_dept_monthly_plans odmp              -- 拠点別月別計画テーブル
        ,(SELECT TO_CHAR(glp.period_year )         AS  period_year -- 年度
                ,TO_CHAR(glp.start_date,'YYYYMM')  AS  year_month  -- 年月
           FROM  gl_sets_of_books  glb                -- 会計帳簿マスタ
                ,gl_periods        glp                -- 会計カレンダテーブル
          WHERE  glb.set_of_books_id        = FND_PROFILE.VALUE('GL_SET_OF_BKS_ID')  -- '1002' 
            AND  glp.period_set_name        = glb.period_set_name
            AND  glp.period_year            = :1    -- 売上計画検索情報リージョン.年度
            AND  glp.adjustment_period_flag = 'N'
         ) get_ym
 WHERE  msp.location_cd(+)    = :2                  -- 売上計画検索情報リージョン.拠点コード
   AND  msp.plan_ym(+)        = get_ym.year_month
   AND  msp.plan_year(+)      = get_ym.period_year  
   AND  odmp.base_code(+)     = :3                  -- 売上計画検索情報リージョン.拠点コード
   AND  odmp.year_month(+)    = get_ym.year_month
   AND  odmp.fiscal_year(+)   = get_ym.period_year
 ORDER BY year_month ASC
   ]]></SQLQuery>
