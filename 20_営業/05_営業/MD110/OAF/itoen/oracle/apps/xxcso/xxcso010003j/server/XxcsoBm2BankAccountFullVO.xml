<ViewObject
   Name="XxcsoBm2BankAccountFullVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="itoen.oracle.apps.xxcso.xxcso010003j.server.XxcsoBm2BankAccountFullVORowImpl"
   ComponentClass="itoen.oracle.apps.xxcso.xxcso010003j.server.XxcsoBm2BankAccountFullVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT XxcsoBankAccountsEO.BANK_ACCOUNT_ID            AS bank_account_id,
       xxcso_010003j_pkg.decode_bm_info(
         xdc.supplier_id
        ,xcm.status
        ,xcm.cooperate_flag
        ,xcm.batch_proc_status
        ,XxcsoBankAccountsEO.BANK_NUMBER
        ,xbav.bank_number
       )                                              AS bank_number,
       xxcso_010003j_pkg.decode_bm_info(
         xdc.supplier_id
        ,xcm.status
        ,xcm.cooperate_flag
        ,xcm.batch_proc_status
        ,XxcsoBankAccountsEO.BANK_NAME
        ,xbav.bank_name
       )                                              AS bank_name,
       xxcso_010003j_pkg.decode_bm_info(
         xdc.supplier_id
        ,xcm.status
        ,xcm.cooperate_flag
        ,xcm.batch_proc_status
        ,XxcsoBankAccountsEO.BRANCH_NUMBER
        ,xbav.bank_num
       )                                              AS branch_number,
       xxcso_010003j_pkg.decode_bm_info(
         xdc.supplier_id
        ,xcm.status
        ,xcm.cooperate_flag
        ,xcm.batch_proc_status
        ,XxcsoBankAccountsEO.BRANCH_NAME
        ,xbav.bank_branch_name
       )                                              AS branch_name,
       xxcso_010003j_pkg.decode_bm_info(
         xdc.supplier_id
        ,xcm.status
        ,xcm.cooperate_flag
        ,xcm.batch_proc_status
        ,XxcsoBankAccountsEO.BANK_ACCOUNT_TYPE
        ,xbav.bank_account_type
       )                                              AS bank_account_type,
       xxcso_010003j_pkg.decode_bm_info(
         xdc.supplier_id
        ,xcm.status
        ,xcm.cooperate_flag
        ,xcm.batch_proc_status
        ,XxcsoBankAccountsEO.BANK_ACCOUNT_NUMBER
        ,xbav.bank_account_num
       )                                              AS bank_account_number,
       xxcso_010003j_pkg.decode_bm_info(
         xdc.supplier_id
        ,xcm.status
        ,xcm.cooperate_flag
        ,xcm.batch_proc_status
        ,XxcsoBankAccountsEO.BANK_ACCOUNT_NAME_KANA
        ,xbav.account_holder_name_alt
       )                                              AS bank_account_name_kana,
       xxcso_010003j_pkg.decode_bm_info(
         xdc.supplier_id
        ,xcm.status
        ,xcm.cooperate_flag
        ,xcm.batch_proc_status
        ,XxcsoBankAccountsEO.BANK_ACCOUNT_NAME_KANJI
        ,xbav.account_holder_name
       )                                              AS bank_account_name_kanji,
       XxcsoBankAccountsEO.BANK_ACCOUNT_DUMMY_FLAG, 
       XxcsoBankAccountsEO.CREATED_BY, 
       XxcsoBankAccountsEO.CREATION_DATE, 
       XxcsoBankAccountsEO.LAST_UPDATED_BY, 
       XxcsoBankAccountsEO.LAST_UPDATE_DATE, 
       XxcsoBankAccountsEO.LAST_UPDATE_LOGIN, 
       XxcsoBankAccountsEO.REQUEST_ID, 
       XxcsoBankAccountsEO.PROGRAM_APPLICATION_ID, 
       XxcsoBankAccountsEO.PROGRAM_ID, 
       XxcsoBankAccountsEO.PROGRAM_UPDATE_DATE, 
       XxcsoBankAccountsEO.DELIVERY_ID
FROM   XXCSO_BANK_ACCOUNTS        XxcsoBankAccountsEO
      ,xxcso_destinations         xdc
      ,xxcso_contract_managements xcm
      ,xxcso_bank_accts_v         xbav
WHERE  xdc.delivery_id            = XxcsoBankAccountsEO.DELIVERY_ID
AND    xcm.contract_management_id = xdc.contract_management_id
AND    xbav.vendor_id(+)          = xdc.supplier_id
   ]]></SQLQuery>
