<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="XxcsoBmAccountInfoSummaryVO"
   OrderBy="account_number"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="itoen.oracle.apps.xxcso.xxcso010003j.server.XxcsoBmAccountInfoSummaryVORowImpl"
   ComponentClass="itoen.oracle.apps.xxcso.xxcso010003j.server.XxcsoBmAccountInfoSummaryVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT   xcav.cust_account_id  AS account_id
        ,xcav.account_number   AS account_number
        ,xcav.party_name       AS party_name
FROM     xxcso_cust_accounts_v xcav
WHERE    (
           (
             xcav.contractor_supplier_code IN (:1, :2, :3)
           )
           OR
           (
             xcav.bm_pay_supplier_code1 IN (:4, :5, :6)
           )
           OR
           (
             xcav.bm_pay_supplier_code2 IN (:7, :8, :9)
           )
         )
  AND    xcav.cust_account_id <> :10
   ]]></SQLQuery>
   <DesignTime>
