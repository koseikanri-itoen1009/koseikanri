<ViewObject
   Name="XxcsoInitBmInfoSummaryVO"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="itoen.oracle.apps.xxcso.xxcso010003j.server.XxcsoInitBmInfoSummaryVORowImpl"
   ComponentClass="itoen.oracle.apps.xxcso.xxcso010003j.server.XxcsoInitBmInfoSummaryVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT  xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,NULL
         ,pv.segment1
        )                                    AS vendor_code
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,xsdc.transfer_commission_type
         ,pvs.bank_charge_bearer
        )                                    AS transfer_commission_type
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,xsdc.bm_payment_type
         ,pvs.attribute4
        )                                    AS bm_payment_type
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,xsdc.inquiry_base_code
         ,pvs.attribute5
        )                                    AS inquiry_base_code
       ,xxcso_util_common_pkg.get_base_name(
          xxcso_010003j_pkg.decode_bm_info(
            xsdc.customer_id
           ,'1'
           ,'1'
           ,'0'
           ,xsdc.inquiry_base_code
           ,pvs.attribute5
          )
         ,TRUNC(xxcso_util_common_pkg.get_online_sysdate)
        )                                    AS inquiry_base_name
       ,SUBSTRB(
          xxcso_010003j_pkg.decode_bm_info(
            xsdc.customer_id
           ,'1'
           ,'1'
           ,'0'
           ,xsdc.party_name
           ,pvs.attribute1
          )
         ,1
         ,80
        )                                    AS vendor_name
       ,SUBSTRB(
          xxcso_010003j_pkg.decode_bm_info(
            xsdc.customer_id
           ,'1'
           ,'1'
           ,'0'
           ,xsdc.party_name_alt
           ,pv.vendor_name_alt
          )
         ,1
         ,320
        )                                    AS vendor_name_alt
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,xsdc.postal_code
         ,pvs.zip
        )                                    AS postal_code
       ,SUBSTRB(
          xxcso_010003j_pkg.decode_bm_info(
            xsdc.customer_id
           ,'1'
           ,'1'
           ,'0'
           ,xsdc.state
           ,pvs.state
          )
         ,1
         ,25
        )                                    AS state
       ,SUBSTRB(
          xxcso_010003j_pkg.decode_bm_info(
            xsdc.customer_id
           ,'1'
           ,'1'
           ,'0'
           ,xsdc.city
           ,pvs.city
          )
         ,1
         ,25
        )                                    AS city
       ,SUBSTRB(
          xxcso_010003j_pkg.decode_bm_info(
            xsdc.customer_id
           ,'1'
           ,'1'
           ,'0'
           ,xsdc.address1
           ,pvs.address_line1
          )
         ,1
         ,35
        )                                    AS address1
       ,SUBSTRB(
          xxcso_010003j_pkg.decode_bm_info(
            xsdc.customer_id
           ,'1'
           ,'1'
           ,'0'
           ,xsdc.address2
           ,pvs.address_line2
          )
         ,1
         ,35
        )                                    AS address2
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,xsdc.address_lines_phonetic
         ,pvs.phone
        )                                    AS address_lines_phonetic
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,NULL
         ,xbav.bank_number
        )                                    AS bank_number
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,NULL
         ,xbav.bank_name
        )                                    AS bank_name
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,NULL
         ,xbav.bank_num
        )                                    AS bank_num
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,NULL
         ,xbav.bank_branch_name
        )                                    AS bank_branch_name
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,NULL
         ,xbav.bank_account_type
        )                                    AS bank_account_type
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,NULL
         ,xbav.bank_account_num
        )                                    AS bank_account_num
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,NULL
         ,xbav.account_holder_name_alt
        )                                    AS account_holder_name_alt
       ,xxcso_010003j_pkg.decode_bm_info(
          xsdc.customer_id
         ,'1'
         ,'1'
         ,'0'
         ,NULL
         ,xbav.account_holder_name
        )                                    AS account_holder_name
       ,xsdc.customer_id                     AS customer_id
FROM    xxcso_sp_decision_custs  xsdc
       ,po_vendors               pv
       ,po_vendor_sites          pvs
       ,xxcso_bank_accts_v       xbav
WHERE   xsdc.sp_decision_customer_id  = :1
AND     xsdc.bm_payment_type         <> '5'
AND     pv.vendor_id(+)               = xsdc.customer_id
AND     pvs.vendor_id(+)              = xsdc.customer_id
AND     xbav.vendor_id(+)             = pvs.vendor_id
AND     xbav.vendor_site_id(+)        = pvs.vendor_site_id
   ]]></SQLQuery>
