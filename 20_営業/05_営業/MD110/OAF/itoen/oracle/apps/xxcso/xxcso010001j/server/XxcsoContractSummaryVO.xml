<ViewObject
   Name="XxcsoContractSummaryVO"
   OrderBy="CONTRACT_NUMBER"
   BindingStyle="Oracle"
   CustomQuery="true"
   RowClass="itoen.oracle.apps.xxcso.xxcso010001j.server.XxcsoContractSummaryVORowImpl"
   ComponentClass="itoen.oracle.apps.xxcso.xxcso010001j.server.XxcsoContractSummaryVOImpl"
   MsgBundleClass="oracle.jbo.common.JboResourceBundle"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT xsdh.sp_decision_number sp_decision_header_num
      ,xcm.contract_number contract_number
      ,xcm.last_update_date last_update_date
      ,xcm.install_account_number install_account_number
      ,xcm.install_party_name install_party_name
      ,xxcso_util_common_pkg.get_lookup_meaning(
                 'XXCSO1_CONTRACT_STATUS',xcm.status,TRUNC(xxcso_util_common_pkg.get_online_sysdate)) status
      ,xsdh.sp_decision_header_id sp_decision_header_id
      ,NULL select_flag
      ,xcm.contract_management_id contract_management_id
      ,xcm.contract_format contract_format
      ,xcm.cooperate_flag cooperate_flag
      ,xcm.status statuscd
FROM   xxcso_contract_managements xcm
      ,xxcso_sp_decision_headers xsdh
WHERE  (
        (
         (:1 IS NOT NULL)
         AND
         (xcm.contract_number LIKE :2)
        )
        OR
        (
         (:3 IS NULL)
        )
       )
       AND
       (
        (
         (:4 IS NOT NULL)
         AND
         (xcm.install_account_number LIKE :5)
        )
        OR
        (
         (:6 IS NULL)
        )
       )
       AND
       (
        (
         (:7 IS NOT NULL)
         AND
         (xcm.install_party_name LIKE :8)
        )
        OR
        (
         (:9 IS NULL)
        )
       )
AND  xcm.sp_decision_header_id = xsdh.sp_decision_header_id
   ]]></SQLQuery>
