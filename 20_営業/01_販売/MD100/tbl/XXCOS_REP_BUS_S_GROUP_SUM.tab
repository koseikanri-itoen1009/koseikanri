COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_S_GROUP_SUM.RESULTS_EMPLOYEE_CODE                 IS  '成績計上者コード';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_S_GROUP_SUM.POLICY_GROUP_CODE                     IS  '政策群コード';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_S_GROUP_SUM.SALE_AMOUNT                           IS  '純売上金額';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_S_GROUP_SUM.BUSINESS_COST                         IS  '営業原価';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_S_GROUP_SUM.CREATED_BY                            IS  '作成者';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_S_GROUP_SUM.CREATION_DATE                         IS  '作成日';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_S_GROUP_SUM.LAST_UPDATED_BY                       IS  '最終更新者';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_S_GROUP_SUM.LAST_UPDATE_DATE                      IS  '最終更新日';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_S_GROUP_SUM.LAST_UPDATE_LOGIN                     IS  '最終更新ﾛｸﾞｲﾝ';
