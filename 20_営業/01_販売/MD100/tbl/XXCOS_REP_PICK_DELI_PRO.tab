COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.ENTRY_NUMBER              IS '伝票NO';
COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.REGULAR_SALE_CLASS_LINE   IS '定番特売区分（明細）';
