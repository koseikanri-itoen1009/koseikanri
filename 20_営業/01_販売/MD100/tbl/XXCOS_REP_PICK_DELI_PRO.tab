COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.CREATION_DATE             IS '作成日';
COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.LAST_UPDATED_BY           IS '最終更新者';
COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.LAST_UPDATE_DATE          IS '最終更新日';
COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.LAST_UPDATE_LOGIN         IS '最終更新ログイン';
COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.REQUEST_ID                IS '要求ID';
COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.PROGRAM_APPLICATION_ID    IS 'コンカレント・プログラム・アプリケーションID';
