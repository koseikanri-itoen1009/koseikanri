COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.SHOP_NAME                 IS '店舗名称';
COMMENT ON COLUMN XXCOS.XXCOS_REP_PICK_DELI_PRO.CONTENT                   IS '入数';
