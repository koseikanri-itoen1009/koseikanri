COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.LINE_NO                      IS '明細NO';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.DLV_INVOICE_CLASS            IS '納品伝票区分';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.PARTY_NUM                    IS '顧客コード';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.PARTY_NAME                   IS '顧客名';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.PERFORMANCE_BY_CODE          IS '成績者コード';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.PERFORMANCE_BY_NAME          IS '成績者名';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.AFTERTAX_SALE                IS '売上金額';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.PRETAX_PAYMENT               IS '税込入金';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.SALE_DISCOUNT                IS '売上値引';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.ITEM_NAME1                   IS '商品名称1';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.QUANTITY1                    IS '数量1';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.ITEM_NAME2                   IS '商品名称2';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.QUANTITY2                    IS '数量2';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.ITEM_NAME3                   IS '商品名称3';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.QUANTITY3                    IS '数量3';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.ITEM_NAME4                   IS '商品名称4';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.QUANTITY4                    IS '数量4';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.ITEM_NAME5                   IS '商品名称5';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.QUANTITY5                    IS '数量5';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.ITEM_NAME6                   IS '商品名称6';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.QUANTITY6                    IS '数量6';
