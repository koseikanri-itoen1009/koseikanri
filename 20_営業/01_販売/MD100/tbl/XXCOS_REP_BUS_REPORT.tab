COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.DELAY_VALID_COUNT            IS '延有効件数';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.DLV_TOTAL_SALE               IS '納品合計純売上';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.DLV_TOTAL_RTN                IS '納品合計返品';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.DLV_TOTAL_DISCOUNT           IS '納品合計値引';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.PERFORMANCE_TOTAL_SALE       IS '成績合計純売上';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.PERFORMANCE_TOTAL_RTN        IS '成績合計返品';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.PERFORMANCE_TOTAL_DISCOUNT   IS '成績合計値引';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.CREATED_BY                   IS '作成者';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.CREATION_DATE                IS '作成日';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.LAST_UPDATED_BY              IS '最終更新者';
