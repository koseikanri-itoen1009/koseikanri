CREATE TABLE XXCOS.XXCOS_REP_BUS_REPORT
(
    RECORD_ID                     NUMBER          NOT NULL,               --レコードID
    DLV_DATE                      DATE,                                   --納品日
    BASE_CODE                     VARCHAR2(4),                            --拠点コード
    BASE_NAME                     VARCHAR2(40),                           --拠点名称
    GROUP_NO                      VARCHAR2(2),                            --グループ番号
    GROUP_IN_SEQUENCE             VARCHAR2(2),                            --グループ内順序
    EMPLOYEE_NUM                  VARCHAR2(5),                            --営業員コード
    EMPLOYEE_NAME                 VARCHAR2(40),                           --営業員氏名
    DEALINGS_CLASS                VARCHAR2(1),                            --取引区分
    DEALINGS_CONTENT              VARCHAR2(8),                            --取引内容
    VISIT_TIME                    VARCHAR2(5),                            --訪問時間
    HHT_INVOICE_NO                VARCHAR2(12),                           --HHT伝票No
    LINE_NO                       NUMBER(2,0),                            --明細NO
    DLV_INVOICE_CLASS             VARCHAR2(1),                            --納品伝票区分
    PARTY_NUM                     VARCHAR2(9),                            --顧客コード
    PARTY_NAME                    VARCHAR2(40),                           --顧客名
    PERFORMANCE_BY_CODE           VARCHAR2(5),                            --成績者コード
    PERFORMANCE_BY_NAME           VARCHAR2(40),                           --成績者名
    AFTERTAX_SALE                 NUMBER,                                 --売上金額
    PRETAX_PAYMENT                NUMBER,                                 --入金金額
    SALE_DISCOUNT                 NUMBER,                                 --売上値引
    ITEM_NAME1                    VARCHAR2(16),                           --商品名称1
    QUANTITY1                     NUMBER(13,2),                           --数量1
    ITEM_NAME2                    VARCHAR2(16),                           --商品名称2
    QUANTITY2                     NUMBER(13,2),                           --数量2
    ITEM_NAME3                    VARCHAR2(16),                           --商品名称3
    QUANTITY3                     NUMBER(13,2),                           --数量3
    ITEM_NAME4                    VARCHAR2(16),                           --商品名称4
    QUANTITY4                     NUMBER(13,2),                           --数量4
    ITEM_NAME5                    VARCHAR2(16),                           --商品名称5
    QUANTITY5                     NUMBER(13,2),                           --数量5
    ITEM_NAME6                    VARCHAR2(16),                           --商品名称6
    QUANTITY6                     NUMBER(13,2),                           --数量6
    DELAY_VISIT_COUNT             NUMBER,                                 --延訪問件数
    DELAY_VALID_COUNT             NUMBER,                                 --延有効件数
    DLV_TOTAL_SALE                NUMBER,                                 --納品合計純売上
    DLV_TOTAL_RTN                 NUMBER,                                 --納品合計返品
    DLV_TOTAL_DISCOUNT            NUMBER,                                 --納品合計値引
    PERFORMANCE_TOTAL_SALE        NUMBER,                                 --成績合計純売上
    PERFORMANCE_TOTAL_RTN         NUMBER,                                 --成績合計返品
    PERFORMANCE_TOTAL_DISCOUNT    NUMBER,                                 --成績合計値引
    CREATED_BY                    NUMBER(15,0)    NOT NULL,               --作成者
    CREATION_DATE                 DATE            NOT NULL,               --作成日
    LAST_UPDATED_BY               NUMBER(15,0)    NOT NULL,               --最終更新者
    LAST_UPDATE_DATE              DATE            NOT NULL,               --最終更新日
    LAST_UPDATE_LOGIN             NUMBER(15,0),                           --最終更新ログイン
    REQUEST_ID                    NUMBER(15,0),                           --要求ID
    PROGRAM_APPLICATION_ID        NUMBER(15,0),                           --コンカレント・プログラム・アプリケーションI
    PROGRAM_ID                    NUMBER(15,0),                           --コンカレント・プログラムID
    PROGRAM_UPDATE_DATE           DATE,                                   --プログラム更新日
    CONSTRAINT XXCOS_REP_BUS_REPORT_PK PRIMARY KEY (RECORD_ID)
    USING INDEX TABLESPACE xxidx2
);
--
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.RECORD_ID                    IS 'レコードID';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.DLV_DATE                     IS '納品日';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.BASE_CODE                    IS '拠点コード';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.BASE_NAME                    IS '拠点名称';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.GROUP_NO                     IS 'グループ番号';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.GROUP_IN_SEQUENCE            IS 'グループ内順序';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.EMPLOYEE_NUM                 IS '営業員コード';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.EMPLOYEE_NAME                IS '営業員氏名';
COMMENT ON COLUMN XXCOS.XXCOS_REP_BUS_REPORT.DEALINGS_CLASS               IS '取引区分';
