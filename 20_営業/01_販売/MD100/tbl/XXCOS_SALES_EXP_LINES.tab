COMMENT ON COLUMN XXCOS.XXCOS_SALES_EXP_LINES.TO_CALCULATE_FEES_FLAG            IS  '手数料計算インタフェース済フラグ'  ;
COMMENT ON COLUMN XXCOS.XXCOS_SALES_EXP_LINES.UNIT_PRICE_MST_FLAG               IS  '単価マスタ作成済フラグ'            ;
COMMENT ON COLUMN XXCOS.XXCOS_SALES_EXP_LINES.INV_INTERFACE_FLAG                IS  'INVインタフェース済フラグ'         ;
