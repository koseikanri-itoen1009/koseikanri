BEGIN FND_FORM_CUSTOM_RULES "272"
  FUNCTION_NAME = "INV_INVSDSUB"
  DESCRIPTION = "コードエラーチェック"
  TRIGGER_EVENT = "WHEN-VALIDATE-RECORD"
  TRIGGER_OBJECT = "SUBINV"
  CONDITION =
 "(SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,1,1) = 'A'\n\
AND (NOT EXISTS (SELECT 1\n\
FROM   HZ_CUST_ACCOUNTS HCA\n\
WHERE  HCA.ACCOUNT_NUMBER = SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,2,4)\n\
AND    HCA.CUSTOMER_CLASS_CODE = 1)\n\
OR LENGTHB(:SUBINV.SECONDARY_INVENTORY_NAME) <> 7))\n\
OR\n\
(SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,1,1) = 'C'\n\
AND (NOT EXISTS (SELECT 1\n\
FROM   HZ_CUST_ACCOUNTS HCA\n\
WHERE  HCA.ACCOUNT_NUMBER = SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,2,4)\n\
AND    HCA.CUSTOMER_CLASS_CODE = 1)\n\
OR (NOT EXISTS (SELECT  1\n\
FROM    PER_PEOPLE_F PPF\n\
WHERE   PPF.EMPLOYEE_NUMBER = SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,6,5)))\n\
OR LENGTHB(:SUBINV.SECONDARY_INVENTORY_NAME) <> 10))\n\
OR\n\
(SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,1,1) = 'V'\n\
AND (NOT EXISTS (SELECT 1\n\
FROM   HZ_CUST_ACCOUNTS HCA\n\
WHERE  HCA.ACCOUNT_NUMBER = SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,2,4)\n\
AND    HCA.CUSTOMER_CLASS_CODE = 1)\n\
OR (SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,6,1) NOT IN ('F','S'))\n\
OR LENGTHB(:SUBINV.SECONDARY_INVENTORY_NAME) <> 6))\n\
OR\n\
(SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,1,1) = 'S'\n\
AND (NOT EXISTS (SELECT 1\n\
FROM   HZ_CUST_ACCOUNTS HCA\n\
WHERE  HCA.ACCOUNT_NUMBER = SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,2,4)\n\
AND    HCA.CUSTOMER_CLASS_CODE = 1)\n\
OR (LENGTHB(:SUBINV.SECONDARY_INVENTORY_NAME) <> 10 AND LENGTHB(:SUBINV.SECONDARY_INVENTORY_NAME) <> 7)))\n\
OR\n\
(SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,1,1) = 'Z'\n\
AND LENGTHB(:SUBINV.SECONDARY_INVENTORY_NAME) <> 1)\n\
OR SUBSTR(:SUBINV.SECONDARY_INVENTORY_NAME,1,1) NOT IN ('A','C','V','S','Z')"
  SEQUENCE = "2"
  CREATED_BY = "-1"
  CREATION_DATE = "2009/02/20"
  OWNER = "ANONYMOUS"
  LAST_UPDATE_DATE = "2009/02/20"
  LAST_UPDATE_LOGIN = "0"
  ENABLED = "Y"
  FIRE_IN_ENTER_QUERY = "N"
  FORM_NAME = "INVSDSUB"
  RULE_TYPE = "F"

